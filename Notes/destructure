const arr =[
  {
    name:'superman',
    age: 50,
    married: false
  }
  , {
    name:'catwoman',
    age: 20,
    married: false
  },
   {
    name:'Shakimanman',
     age: 25,
    married: true
  },
  {
    name:'superman',
    age: 40
  }
]


const [a, b] = arr
console.log(a,b)

arr1 = [1,2,3,4,5,6];

const [,,c,d]= arr1
console.log(c,d)

const [, ...rest]= arr1
console.log(rest)

object destructure:

const user = {name:'sumit', role: 'user'}

const {name} =  user;

console.log(name)
